{
  "name": "plp-registry",
  "version": "0.0.2",
  "description": "PLP Registry CLI - Manage PLP server registration, compliance testing, and listing on plp.pub",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "plp": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "plp",
    "prompt",
    "library",
    "protocol",
    "registry",
    "compliance",
    "testing",
    "validation",
    "management"
  ],
  "author": "GoReal.AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gorealai/plp.git",
    "directory": "tools/registry"
  },
  "bugs": {
    "url": "https://github.com/gorealai/plp/issues"
  },
  "homepage": "https://github.com/gorealai/plp#readme",
  "dependencies": {
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
